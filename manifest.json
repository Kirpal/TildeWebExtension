{
	"name": "Tilde",
	"version": "6.1.1",
	"minimum_chrome_version": "37",
	"manifest_version": 2,
	"description": "Functional and sexy minimal browser startpage.",
	"icons": {
		"128": "icon_128.png",
		 "48": "icon_48.png",
		 "16": "icon_16.png"
	},
	"permissions": ["storage"],
	"chrome_url_overrides":
	{
		"newtab": "newtab.html"
	},
	"options_ui": {
		"page": "options.html"
	},
	"content_security_policy": "script-src 'self' https://duckduckgo.com/; object-src 'self'"
}
